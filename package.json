{
  "name": "check-my-diary",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run css-build && node server.js",
    "start:dev": "yarn build && DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
    "build": "yarn run css-build",
    "css-build": "./bin/build-css",
    "clean": "rm -rf public/* .port.tmp *.log build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "NODE_ENV=test mocha test/*.spec.js test/**/*.spec.js"
  },
  "engines": {
    "node": ">= 8.11.3 < 9.0.0",
    "npm": ">=5.6.0 < 6.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "node_modules/*",
      "test/*"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "bunyan-format": "0.2.1",
    "bunyan-request-logger": "2.0.0",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "csurf": "1.9.0",
    "debug": "3.1.0",
    "dotenv": "6.0.0",
    "ejs": "2.6.1",
    "express": "4.16.3",
    "express-request-id": "1.4.0",
    "govuk-elements-sass": "3.1.3",
    "govuk_frontend_toolkit": "7.6.0",
    "govuk_template_jinja": "0.24.1",
    "helmet": "3.13.0",
    "http-errors": "1.7.0",
    "moment": "2.22.2",
    "morgan": "1.9.0",
    "node-sass-middleware": "0.11.0",
    "passport": "0.4.0",
    "sass-middleware": "0.0.3"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.4.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.3",
    "sinon": "6.1.5"
  },
  "optionalDependencies": {
    "hoek": "5.0.4"
  }
}
