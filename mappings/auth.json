{
  "request": {
    "method": "POST",
    "url": "/oauth/token",
    "bodyPatterns": [
      {
        "contains": "PMASSEY_GEN"
      },
      {
        "contains": "password123456"
      }
    ]
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "access_token": "RW_TOKEN",
      "token_type": "bearer",
      "refresh_token": "refreshToken",
      "expires_in": 599,
      "scope": "read write",
      "internalUser": true
    }
  }
}
